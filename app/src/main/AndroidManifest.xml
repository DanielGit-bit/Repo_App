        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".editar_usuario"></activity>
        <activity android:name=".menu_logeadoActivity" />
        <activity android:name=".carrito_comprasActivity" />
        <activity android:name=".agregar_metodo_pagoActivity" />
        <activity android:name=".registrarse_Activity" />
        <activity android:name=".loginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>